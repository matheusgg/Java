
SELECT t0.ID_REPRESENTANTE, t0.DTYPE, t0.NR_CPF, t0.DT_FIM_VIGENCIA, t0.DT_INCLUSAO, t0.DT_INI_VIGENCIA, t0.DS_EMAIL, t0.DS_REGISTRO_CREA,
